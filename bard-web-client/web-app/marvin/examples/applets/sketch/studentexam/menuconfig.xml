<?xml version="1.0" encoding="UTF-8"?>
<customization active="studentexam">
    <scheme id="studentexam">
    <bundle base="examples.applets.sketch.studentexam.studentexam" explicit="true"/>
        <toolbar id="general" name="%toolbar.general.name" anchor="north" index="0">
            <item id="selectModeGroup"/>
            <item id="eraseMode"/>
            <separator id="sep-2"/>
            <item id="undo"/>
            <item id="redo"/>
            <separator id="sep-3"/>
            <item id="cut"/>
            <item id="copy"/>
            <item id="paste"/>
            <separator id="sep-4"/>
            <item id="zoomIn"/>
            <item id="zoomOut"/>
            <item id="zoomTool"/>
            <separator id="sep-5"/>
            <item id="helpContents"/>
        </toolbar>
        <toolbar id="atoms" name="%toolbar.atoms.name" anchor="east">
            <item id="periodicSystem"/>
            <item id="atom.H"/>
            <item id="atom.C"/>
            <item id="atom.N"/>
            <item id="atom.O"/>
            <item id="atom.S"/>
            <item id="atom.F"/>
            <item id="atom.P"/>
            <item id="atom.Cl"/>
            <item id="atom.Br"/>
            <item id="atom.I"/>
        </toolbar>
        <toolbar id="tools" name="%toolbar.tools.name" anchor="west">
            <item id="bondGroup"/>
            <item id="insertChain"/>
            <item id="insertText"/>
            <item id="reactionArrow0"/>
            <item id="createSGroup"/>
            <item id="bracesGroup"/>
            <item id="basicGraphicsGroup"/>
            <item id="increaseCharge"/>
            <item id="decreaseCharge"/>
            <item id="lonePairGroup"/>
            <item id="freeRadicalGroup"/>
        </toolbar>
        <toolbar id="mytemplates" name="%toolbar.mytemplates.name" anchor="south"/>
        <popup id="edit" name="%popup.edit.name">
            <item id="cut"/>
            <item id="copy"/>
            <item id="copyAs"/>
            <item id="paste"/>
            <item id="selectAll"/>
            <item id="delete"/>
            <separator id="sep-1"/>
            <item id="createSGroup"/>
            <item id="ungroupSGroup"/>
            <item id="contractSGroup"/>
            <item id="expandSGroup"/>
            <item id="mergeBrackets"/>
            <separator id="sep-2"/>
            <menu id="structure-add-menu" name="%menu.structure-add-menu.name">
                <item id="hydrogenize"/>
                <item id="mapAtoms"/>
                <item id="attachData"/>
                <item id="setAbsoluteStereo"/>
                <item id="addMulticenter"/>
                <item id="createMarkush"/>
            </menu>
            <menu id="structure-remove-menu" name="%menu.structure-remove-menu.name">
                <item id="dehydrogenize"/>
                <item id="unmapAtoms"/>
                <item id="removeData"/>
                <item id="removeAbsoluteStereo"/>
            </menu>
            <item id="editData"/>
            <separator id="sep-3"/>
            <item id="atomLinkNode"/>
            <menu id="attribute-menu" name="%menu.attribute-menu.name">
                <item id="rlogic"/>
            </menu>
            <item id="groupReactionFragment"/>
            <item id="splitReactionFragment"/>
            <separator id="sep-4"/>
            <menu id="transform-menu" name="%menu.transform-menu.name">
                <item id="normalMode"/>
                <item id="zoomMode"/>
                <item id="rotate3DMode"/>
                <item id="dragSelection"/>
                <item id="rotateSelection"/>
                <item id="rotateSelection3D"/>
                <item id="switchSelectionTransformation"/>
                <separator id="sep-1"/>
                <item id="flipGroup"/>
                <item id="flipHorizontally"/>
                <item id="flipVertically"/>
                <separator id="sep-2"/>
                <item id="mirrorHorizontally"/>
                <item id="mirrorVertically"/>
            </menu>
            <item id="format"/>
            <item id="addToMyTemplates"/>
            <separator id="sep-5"/>
            <item id="helpContents"/>
            <item id="about"/>
        </popup>
        <popup id="object" name="%popup.object.name">
            <item id="moveToFront"/>
            <item id="moveToBack"/>
            <separator id="sep-1"/>
            <item id="cut"/>
            <item id="copy"/>
            <item id="paste"/>
            <item id="delete"/>
            <separator id="sep-2"/>
            <menu id="transform-menu" name="%menu.transform-menu.name">
                <item id="dragSelection"/>
                <item id="rotateSelection"/>
                <item id="rotateSelection3D"/>
                <item id="switchSelectionTransformation"/>
                <separator id="sep-1"/>
                <item id="flipHorizontally"/>
                <item id="flipVertically"/>
                <separator id="sep-2"/>
                <item id="mirrorHorizontally"/>
                <item id="mirrorVertically"/>
            </menu>
            <item id="format"/>
            <item id="addToMyTemplates"/>
        </popup>
        <popup id="bond" name="%popup.bond.name">
            <menu id="bond-type-menu" name="%menu.bond-type-menu.name">
                <item id="bondType.single"/>
                <item id="bondType.double"/>
                <item id="bondType.triple"/>
                <item id="bondType.aromatic"/>
                <separator id="sep-1"/>
                <item id="bondType.singleUp"/>
                <item id="bondType.singleDown"/>
                <item id="bondType.singleEither"/>
                <item id="bondType.cisOrTrans"/>
                <item id="bondType.cisOrTransOrUnspec"/>
                <separator id="sep-2"/>
                <item id="bondType.singleOrDouble"/>
                <item id="bondType.singleOrAromatic"/>
                <item id="bondType.doubleOrAromatic"/>
                <item id="bondType.any"/>
                <separator id="sep-3"/>
                <item id="bondType.coord"/>
            </menu>
            <separator id="bond-sep-1"/>
            <menu id="bond-topology-menu" name="%menu.bond-topology-menu.name">
                <item id="bondTopology.ignore"/>
                <item id="bondTopology.inRing"/>
                <item id="bondTopology.inChain"/>
            </menu>
            <menu id="bond-reacting-center-menu" name="%menu.bond-reacting-center-menu.name">
                <item id="bondReactingCenter.none"/>
                <item id="bondReactingCenter.center"/>
                <item id="bondReactingCenter.makeOrBreak"/>
                <item id="bondReactingCenter.change"/>
                <item id="bondReactingCenter.makeAndChange"/>
                <item id="bondReactingCenter.notCenter"/>
            </menu>
            <item id="bondStereoSearch"/>
            <separator id="bond-sep-2"/>
            <menu id="bond-align-menu" name="%menu.bond-align-menu.name">
                <item id="alignBond.horizontal"/>
                <item id="alignBond.vertical"/>
            </menu>
            <separator id="bond-sep-3"/>
            <item id="cut"/>
            <item id="copy"/>
            <item id="copyAs"/>
            <item id="paste"/>
            <item id="delete"/>
            <separator id="sep-1"/>
            <item id="createSGroup"/>
            <item id="ungroupSGroup"/>
            <item id="contractSGroup"/>
            <item id="expandSGroup"/>
            <item id="mergeBrackets"/>
            <separator id="sep-2"/>
            <menu id="structure-add-menu" name="%menu.structure-add-menu.name">
                <item id="hydrogenize"/>
                <item id="mapAtoms"/>
                <item id="attachData"/>
                <item id="setAbsoluteStereo"/>
                <item id="addMulticenter"/>
                <item id="createMarkush"/>
            </menu>
            <menu id="structure-remove-menu" name="%menu.structure-remove-menu.name">
                <item id="dehydrogenize"/>
                <item id="unmapAtoms"/>
                <item id="removeData"/>
                <item id="removeAbsoluteStereo"/>
            </menu>
            <item id="editData"/>
            <separator id="sep-3"/>
            <menu id="transform-menu" name="%menu.transform-menu.name">
                <item id="dragSelection"/>
                <item id="rotateSelection"/>
                <item id="rotateSelection3D"/>
                <item id="switchSelectionTransformation"/>
                <separator id="sep-1"/>
                <item id="flipGroup"/>
                <item id="flipHorizontally"/>
                <item id="flipVertically"/>
                <separator id="sep-2"/>
                <item id="mirrorHorizontally"/>
                <item id="mirrorVertically"/>
            </menu>
            <item id="format"/>
            <item id="addToMyTemplates"/>
        </popup>
    </scheme>
</customization>
